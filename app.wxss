/**app.wxss**/
@import 'asset/iconfont/icon.wxss';

page {
  width: 100%;
  height: 100%;
}

.container {
  height: 100%;
  width: 100%;
  /* display: flex; */
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
  font-family: '微软雅黑';
  background: #f3f3f3;
  position: relative;

}
.container-content{
  height: 100%;
  width: 100%;
}
.record-wrap{
width: 100%;
height: 100%;
position: absolute;
top:0;
left: 0;
}

.figure {
  width: 312rpx;
  height: 398rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 1px 10px 0px rgba(12, 3, 6, 0.2);
  border-radius: 16rpx;
  display: inline-block;
  margin-right: 20rpx;
}

.figure .img-wrap {
  width: 312rpx;
  height: 234rpx;
  border-radius: 16rpx;
  overflow: hidden;
}
.figure .img-wrap image{
  width: 100%;
  height: 100%;
}
.figure .figcaption {
  width: 100%;
  height: calc(100% - 234rpx);
  padding: 8rpx 16rpx;
  box-sizing: border-box;
}

.good-name {
  height:40rpx;
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
display: block;
  overflow: hidden;
 
text-overflow:ellipsis;
white-space: nowrap;
}

.good-discribe {
  display:flex;
  width: 100%;
  height: 55rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(162, 162, 162, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space:pre-line;
  margin-bottom: 10rpx;
}

.price {
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(203, 0, 0, 1);
  position: relative;
  top: -16rpx;
  display: inline-block;
  height: 28rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.picker {
  font-size: 26rpx;
}

.icon-Magnifier:before {
  content: "\e62d";
}

/* iocnfont结束 */

.top-serach .search-content {
  height: 68rpx;
  width: 100%;
}

.area-wrap {
  display: inline-block;
  width: 115rpx;
  height: 100%;
  line-height: 68rpx;
  text-align: center;
  position: relative;
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  text-indent: 10rpx;
}

.area-wrap::after {
  content: "";
  width: 1px;
  height: 30rpx;
  background: rgba(102, 102, 102, 1);
  position: absolute;
  top: 19rpx;
  right: 0;
}

.input-wrap {
  width: calc(100% - 115rpx);
  height: 100%;
  line-height: 68rpx;
  display: inline-block;
  float: right;
  padding-left: 15rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(218, 218, 218, 1);
  border: 1px solid transparent;
  box-sizing: border-box;
}

.input-wrap input {
  line-height: 100%;
  width: 395rpx;
  height: 100%;
  display: inline-block;
  color:rgba(102,102,102,1);
}

.input-wrap text {
  float: left;
  margin-right: 5rpx;
  color: #333;
}

.top-serach {
  width: 100%;
  height: 188rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 4px 4px 10px 0px rgba(130, 130, 130, 0.12);
  padding: 20rpx 38rpx;
  border: 1px solid transparent;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  position: relative;
  z-index: 99999;
}

.screening-condition-wrap {
  height: 60rpx;
  width: 100%;
  margin-top: 20rpx;
}

.screening-condition-wrap .screening-item {
  width: 25%;
  float: left;
  height: 100%;
  line-height: 60rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  position: relative;
}

.screening-condition-wrap .screening-item::after {
  content: '';
  width: 1px;
  height: 40rpx;
  position: absolute;
  top: 10rpx;
  right: 0;
  background: rgba(243, 243, 243, 1);
}

.screening-condition-wrap .screening-item:last-child::after {
  background: transparent;
}

.screening-item-mun-content {
  /* padding: 10rpx 0; */
  background: #fff;
}

.screening-item.active {
  color: #409eff;
}

.iconfont.icon-jiantou {
  color: #409eff;
}

.screen-condition {
  padding: 10rpx 30rpx;
  border-top: 1px solid rgba(0, 0, 0, 0.2);
}

.screen-condition .condition-item {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.icon.iconfont.icon-duihao {
  float: right;
  color: #409eff;
}

.screen-condition.sales-condition {
  height: 110rpx;
}

.custom-button {
  background: #409eff;
  color: #fff;
  padding: 6rpx 10rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
}

.condition-title {
  display: block;
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 30rpx;
}

.price-condition-slide {
  margin-bottom: 50rpx;
}

.price-condition-slide.hotel-screen {
  margin-bottom: 25rpx;
}

.screen-span-wrap .screen-span {
  width: 208rpx;
  height: 58rpx;
  border-radius: 29rpx;
  line-height: 58rpx;
  text-align: center;
  display: inline-block;
  margin-bottom: 20rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  background: rgba(243, 243, 243, 1);
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
}

.screen-span-wrap .screen-span:nth-child(2),
.screen-span-wrap .screen-span:nth-child(5) {
  margin: 0 30rpx;
}

.screen-span-wrap .screen-span.active {
  border: 1px solid rgba(64, 158, 255, 1);
  background: #fff;
  line-height: 56rpx;
}

.more-condition-options {
  height: 98rpx;
  line-height: 98rpx;
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
}

.more-condition-options .more-condition-options-item {
  height: 100%;
  width: 50%;
  float: left;
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0rpx 2rpx 0px rgba(88, 88, 88, 0.28);
}

.more-condition-options .more-condition-options-item:last-child {
  background: rgba(64, 158, 255, 1);
  box-shadow: 0px -1rpx 2rpx 0px rgba(88, 88, 88, 0.28);
  color: #fff;
}

.no-limit {
  height: 30rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(64, 158, 255, 1);
  display: block;
  margin-top: 10rpx;
}

.hot-good-item {
  width: 690rpx;
  height: 272rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 4rpx 4rpx 10rpx 0px rgba(130, 130, 130, 0.12);
  border-radius: 16rpx;
  margin: 0 auto 20rpx;
  overflow: hidden;
}

.hot-good-item .img-wrap {
  width: 204rpx;
  height: 272rpx;
  float: left;
  overflow: hidden;

/* pages/DetailsPage/index.wxss */
.van-popup--bottom{
  bottom: 98rpx !important;
}
.MerPictures{
  width: 100%;
  height: 370rpx;
  position: relative;
}
.MerPictures>image{
  width: 100%;
  height: 100%;
}
.PicturesNumber{
  width: 110rpx;
  height: 40rpx;
  border-radius: 20rpx;
  color: #fff;
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: center;
  position: absolute;
  right: 30rpx;
  bottom: 46rpx;
  background:rgba(0,0,0,.6);
}

.MerContent{
  width:calc(100% - 60rpx);
  /* height:230rpx; */
  padding:30rpx;
  background-color:#fff;
  border-radius:16rpx;
  margin-top:-20rpx;
  position:relative;
}
.ContentTitle{
  font-size: 32rpx;
  color: #333333;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
}
.ContentInfo{
  display: flex;
  vertical-align: middle;
  margin-top: 24rpx;
}
.ContentInfo .ContentInfo_neirongTitle{
  color: #303133;
  font-size: 24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  display: inline-block;
}
.ContentInfo .ContentInfo_neirong{
  display: inline-block;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(96,98,102,1);
}
.ContentDay view:nth-child(1){
  color: #303133;
  font-size: 24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  display: inline-block;
}
.ContentDay view:nth-child(2){
  display: inline-block;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(96,98,102,1);
}
page{
  width: 100%;
  height: 100%;
  background-color: #F3F3F3;
}
.container{
  width: 100%;
  height: auto;
  background-color: #F3F3F3;
}
.CollectionAndReserve{
  width: 100%;
  height: 98rpx;
  position: fixed;
  bottom:0;
  display: flex;
  z-index:1000;

}
.Collection{
  width: 239rpx;
  height: 98rpx;
  display: inline-block;
  background-color: #fff;
  color: #666666;
  font-size: 28rpx;
  text-align: center;
  line-height: 98rpx;
}
.Reserve{
  width: 510rpx;
  height: 98rpx;
  display: inline-block;
  background-color: #409EFF;
}
.Reserve button{
  width: 100%;
  height: 100%;
  background-color: #409EFF;
  outline: none;
  border: none;
  text-align: center;
  line-height: 98rpx;
  color: #fff;
  font-size: 32rpx;
}




.left{
  width:100%;
  height:80%;
  background-color: #fff;
  margin-top: 20rpx;
}
.left scroll-view{
  height:100%;
  display:flex;
  justify-content: center;
}
.left view{
  padding:10px;
  text-align:center;
  margin-left:25rpx;
  color:#666666;
  width:100rpx;
  display:inline-block;
  font-size: 24rpx;
}
.leftScroll{
  width: 90%;
  margin-left: 5%;
  border-bottom: 1px solid #F3F3F3;
}
.right{
  flex:1;
  height:1160rpx;
  padding-bottom:98rpx;
  background-color: #fff;
}
.right scroll-view{
  height:100%;
}

.active-tag{
  color: #409EFF !important;
}
.list_center{
  width: 92%;
  margin-left: 4%;
}
#list_1{
  height: 600rpx;
}
#list_2{
  height: 600rpx;
}
#list_1{
  height: 600rpx;
}
#list_2{
  height: 600rpx;
}
.list_1_title{
  line-height: 92rpx;
  font-size:36rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.list_small_title{
  line-height: 45rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.list_introduce{
  line-height: 45rpx;
